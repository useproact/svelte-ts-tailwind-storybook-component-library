{
	"name": "@newwwton/svelte-tailwind-component",
	"version": "0.0.1",
	"description": "Svelte component starter template",
	"author": "Jerric Lyns John <jerriclynsjohn@outlook.com>",
	"svelte": "src/index.ts",
	"module": "dist/index.mjs",
	"main": "dist/index.js",
	"scripts": {
		"build": "NODE_ENV=production rollup -c",
		"dev": "rollup -c",
		"prepublishOnly": "npm run build",
		"format": "prettier --write ./src/**/*.{ts,js,svelte,md,html,css,json}",
		"prelint": "npm run format",
		"lint": "eslint ./src/**/*.{ts,js,svelte}",
		"lint:fix": "eslint --fix ./src/**/*.{ts,js,svelte}",
		"pretest": "npm run lint"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^2.3.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-typescript": "^5.0.2",
		"@tsconfig/svelte": "^1.0.10",
		"@typescript-eslint/eslint-plugin": "^4.0.1",
		"@typescript-eslint/parser": "^4.0.1",
		"autoprefixer": "^9.8.6",
		"cssnano": "^4.1.10",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.8.1",
		"eslint-config-airbnb-typescript": "^10.0.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-svelte3": "^2.7.3",
		"husky": "^4.3.0",
		"lint-staged": "^10.3.0",
		"postcss-cli": "^7.1.2",
		"postcss-import": "^12.0.1",
		"prettier": "^2.1.1",
		"prettier-plugin-svelte": "^1.2.1",
		"rollup": "^2.26.10",
		"rollup-plugin-postcss": "^3.1.8",
		"rollup-plugin-svelte": "^6.0.0",
		"svelte": "^3.24.1",
		"svelte-check": "^1.0.36",
		"svelte-preprocess": "^4.2.1",
		"tailwindcss": "^1.8.3",
		"typescript": "^4.0.2"
	},
	"files": [
		"src",
		"dist"
	],
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/useproact/svelte-ts-tailwind-storybook-component-library.git"
	},
	"keywords": [
		"Svelte",
		"Tailwind",
		"Typescript",
		"Storybook",
		"Component"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/useproact/svelte-ts-tailwind-storybook-component-library/issues"
	},
	"homepage": "https://github.com/useproact/svelte-ts-tailwind-storybook-component-library#readme",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,svelte,md,html,css,json}": "prettier --write",
		"*.{js,svelte}": "eslint --cache --fix"
	}
}
